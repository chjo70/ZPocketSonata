            <listAttribute key="BLD::Info|Tools"/>
            <stringAttribute key="BLD::Info|redirRoot" value=""/>
            <stringAttribute key="BLD::Info|defaultSpec" value="default"/>
            <listAttribute key="BLD::Info|GlobalMacros">
                <stringAttribute value="VSB_DIR"/>
                <stringAttribute value="BUILD_SPEC_DKM"/>
                <stringAttribute value="DEBUG_MODE_DKM"/>
                <stringAttribute value="BUILD_SPEC_RTP"/>
                <stringAttribute value="DEBUG_MODE_RTP"/>
            </listAttribute>
            <stringAttribute key="BLD::Info|GlobalMacro|VSB_DIR" value="$(PRJ_ROOT_DIR)/../VSourceBuild"/>
            <stringAttribute key="BLD::Info|GlobalMacro|BUILD_SPEC_DKM" value="!"/>
            <stringAttribute key="BLD::Info|GlobalMacro|DEBUG_MODE_DKM" value="*"/>
            <stringAttribute key="BLD::Info|GlobalMacro|BUILD_SPEC_RTP" value="!"/>
            <stringAttribute key="BLD::Info|GlobalMacro|DEBUG_MODE_RTP" value="*"/>
        </mapAttribute>
    </attributes>
