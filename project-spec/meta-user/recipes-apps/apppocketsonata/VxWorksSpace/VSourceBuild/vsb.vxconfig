config ANYTHING
    bool
    default y
    help
        This is a hidden option that is always set, used for choice dependencies

# The following hidden options can be configured for specific ARCH or CPU conditions
# but must remain only in this file or circular dependencies will result 
# no linkage is allowed to any specific layer version.

#  uncomment ALLCOMP to allow the use of compat 69 and non compat 69 compilers for LP64 BSPs
#config ALLCOMP
#     bool
#     default y
#     depends on LP64

#config FORCE_COMPAT69
#     bool
#     default y

#config FORCE_COMPAT7
#     bool
#     default y



# Note if the installed compilers do not support COMPAT69 then disable the COMPAT69
# if the installed compilers do not support !COMPAT69 then lock the  selection to COMPAT69
menu  "Compatibility"
    visible if COMPAT69_SUPPORTED && !COMPAT69_REQUIRED  && !FORCE_COMPAT69 && !FORCE_COMPAT7 && !REQUIRES_VX7_COMPATIBLE
    
config COMPAT69
    bool  "Enable 6.9 Compatible Mode"  
    default y if COMPAT69_DEF || COMPAT69_REQUIRED || FORCE_COMPAT69
    depends on COMPAT69_SUPPORTED && !REQUIRES_VX7_COMPATIBLE && !FORCE_COMPAT7
    help
        This option enables 6.9 Compatibility Mode in the VxWorks kernel.
        

endmenu

